{
  "name": "react-trip-date",
  "version": "0.0.3",
  "description": "date-picker and range-picker for React applications",
  "author": "samsam_ahmadi",
  "license": "GPL-3.0",
  "repository": "samsam-ahmadi/react-trip-date",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "start": "start-storybook -p 6006",
    "prepare": "yarn run build",
    "deploy": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook -c .storybook -o storybook-static",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "polished": "^3.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.2.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@types/storybook__addon-actions": "^5.2.0",
    "@types/storybook__addon-knobs": "^5.2.0",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "dayjs": "^1.8.16",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jalaliday": "^2.2.4",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.4"
  },
  "files": [
    "dist"
  ]
}
